{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"lib": [
			"ESNext",
			"ESNext.AsyncIterable",
			"DOM",
			"DOM.Iterable"
		],
		"moduleResolution": "node",
		"noEmit": true,
		"skipLibCheck": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"jsx": "preserve",
		"allowJs": false,
		"checkJs": false,
		"removeComments": true,
		"strict": true,
		"newLine": "lf",
		"baseUrl": ".",
		"types": [
			"node"
		]
	}
}
